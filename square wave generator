#define DRAWER_PIN 9

// Function to open the lid of the box. Argument is the servo we wish to move.
void open_drawer(int drawer_pin);

int x;

void setup() {
  pinMode(DRAWER_PIN, OUTPUT);
  Serial.begin(9600);
}


void loop() {
//  while(Serial.available() ==0){
