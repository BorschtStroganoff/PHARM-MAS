  open_drawer(DRAWER_PIN);
//  delay(2000);
}

void open_drawer(int drawer_pin)
{
  digitalWrite(drawer_pin, HIGH);
  delay(200);
  digitalWrite(drawer_pin,LOW);
  delay(800);
}

